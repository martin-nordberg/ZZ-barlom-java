{
  "name": "Barlom-GDB-Console",
  "version": "0.0.0",
  "description": "Simple snabbdom starter repository",
  "main": "main.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/martin-nordberg/barlom"
  },
  "author": "Martin E. Nordberg III",
  "dependencies": {
    "snabbdom": "^0.2.0"
  },
  "devDependencies": {
    "babel": "^5.2.13",
    "babelify": "^6.0.2",
    "browserify": "^11.0.0",
    "watchify": "^3.3.0"
  },
  "scripts": {
    "build": "browserify webapp/scripts/main.js -t [ babelify --stage 1 ] --outfile webapp/app.js",
    "watch": "watchify webapp/scripts/main.js -t babelify --outfile webapp/app.js"
  }
}
