{{! Browse tab panel of the Schema page }}

<div class="tab-pane active" id="browseTab">

    {{#with browseTabEntries}}

        {{#each browseSections}}

            <h4>{{title}}</h4>

            {{#each elements}}
                <grestler-element-link element="{{.}}"
                                       targetElementName="schemaPageBrowsedElement"></grestler-element-link>
                <br>
            {{/each}}

            <!-- TODO: different add link for each section -->
            <a class="elementlink add Package" href="#" on-click="addPackageClicked">Add a package</a>

        {{/each}}

    {{/with}}

</div>
